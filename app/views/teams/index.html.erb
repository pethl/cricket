<p id="notice"><%= notice %></p>

<h1>Listing Teams</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Institute</th>
      <th>Year</th>
      <th>Rank</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.name %></td>
        <td><%= team.institute %></td>
        <td><%= team.year %></td>
        <td><%= team.rank %></td>
        <td><%= link_to 'Show', team %></td>
        <td><%= link_to 'Edit', edit_team_path(team) %></td>
        <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team', new_team_path %> | <%= link_to 'Home', root_url %>
